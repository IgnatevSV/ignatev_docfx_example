{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**.sln"],
          "exclude": ["**/obj/**", "**/bin/**", "_site/**"]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": [
          "docfx_notes/**.md",
          "dialogues_system_description/**.yml",
          "dialogues_system_description/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "dialogues_system_description/images/**",
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "DocFX research-training project",
      "_appFooter": "DocFX research-training project 2017",
      "_enableSearch": true,
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/icon.png"
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
